{
    "modulesContent": {
        "$edgeAgent": {
            "properties.desired": {
                "modules": {
                    "producer": {
                        "settings": {
                            "image": "mill5james/producer:latest",
                            "createOptions": "{}"
                        },
                        "type": "docker",
                        "version": "1.0",
                        "status": "running",
                        "restartPolicy": "always"
                    },
                    "consumer": {
                        "settings": {
                            "image": "mill5james/consumer:latest",
                            "createOptions": "{}"
                        },
                        "type": "docker",
                        "env": {
                            "EnableMethod": {
                                "value": "false"
                            },
                            "EnableMessage": {
                                "value": "true"
                            },
                            "EnableHistogram": {
                                "value": "true"
                            }
                        },
                        "version": "1.0",
                        "status": "running",
                        "restartPolicy": "always"
                    }
                },
                "runtime": {
                    "settings": {
                        "minDockerVersion": "v1.25"
                    },
                    "type": "docker"
                },
                "schemaVersion": "1.0",
                "systemModules": {
                    "edgeAgent": {
                        "settings": {
                            "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
                            "createOptions": "{}"
                        },
                        "type": "docker",
                        "env": {
                            "UpstreamProtocol": {
                                "value": "MQTT"
                            }
                        }
                    },
                    "edgeHub": {
                        "settings": {
                            "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
                            "createOptions": "{}"
                        },
                        "type": "docker",
                        "env": {
                            "UpstreamProtocol": {
                                "value": "MQTT"
                            }
                        },
                        "status": "running",
                        "restartPolicy": "always"
                    }
                }
            }
        },
        "$edgeHub": {
            "properties.desired": {
                "routes": {
                    "consumerToProducer": "FROM /messages/modules/consumer/outputs/GetTimeMessage INTO BrokeredEndpoint(\"/modules/producer/inputs/GetTimeMessage\")",
                    "producerToConsumer": "FROM /messages/modules/producer/outputs/GetTimeMessage INTO BrokeredEndpoint(\"/modules/consumer/inputs/GetTimeMessage\")"
                },
                "schemaVersion": "1.0",
                "storeAndForwardConfiguration": {
                    "timeToLiveSecs": 7200
                }
            }
        },
        "producer": {
            "properties.desired": {}
        },
        "consumer": {
            "properties.desired": {}
        }
    }
}